    <div class="container container-custom">
      <div class = "chatty-template">
        <a class="link" href="./room.html">
        <div class="row">
          <div class = "col-xs-6">
            <img class="program-img" src="./img/WestworldLogo.jpg" />
          </div>
          <div class = "description col-xs-6">
            <div class = "row channel-name">
              <p>HBO</p><span> - </span>
            </div>
            <div class = "row program-name">
              <p>Westworld</p>
            </div>
            <div class="time">
              <div class = "row program-time">
                <p>12:00am PST</p>
              </div>
              <div class = "row program-viewer">
                <p>Live Viewers: </p> <span id="viewerNum">1754</span>
              </div>
            </div>
            <div class="collect">
              <a href="#">
                <img id="collect" src="./img/star_gold_small.png"/>
              </a>
            </div>
          </div>
        </div>
        </a>
      </div>

      <!-- test for handlebars -->
